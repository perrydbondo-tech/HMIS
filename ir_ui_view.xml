<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="default_list_view_fo_749f7853-bf54-452d-8428-fffa07f252a8" model="ir.ui.view" context="{'studio': True}">
    <field name="arch" type="xml">
      <list default_order="x_studio_priority desc,x_studio_sequence asc,id desc">
        <field name="x_studio_sequence" widget="handle"/>
        <field name="x_name"/>
        <field name="x_studio_partner_id"/>
        <field name="x_studio_user_id" widget="many2one_avatar_user"/>
        <field name="x_studio_currency_id" invisible="1"/>
        <field name="x_studio_value" sum="Total"/>
        <field name="x_studio_tag_ids" widget="many2many_tags" options="{'color_field': 'x_color'}"/>
        <field name="x_color" widget="color_picker"/>
      </list>
    </field>
    <field name="model">x_appointment</field>
    <field name="name">Default list view for x_appointment</field>
    <field name="type">list</field>
  </record>
  <record id="default_form_view_fo_3d2a361e-1985-49ca-9322-c0bf422b74a7" model="ir.ui.view" context="{'studio': True}">
    <field name="arch" type="xml">
      <form>
        <header>
          <field name="x_studio_stage_id" widget="statusbar" options="{'clickable': '1'}"/>
        </header>
        <sheet string="Appointment">
          <field name="x_studio_kanban_state" widget="state_selection"/>
          <widget name="web_ribbon" text="Archived" bg_color="text-bg-danger" invisible="x_active"/>
          <field name="x_active" invisible="1"/>
          <field class="oe_avatar" widget="image" name="x_studio_image"/>
          <div class="oe_title">
            <h1>
              <field name="x_name" required="1" placeholder="Name..."/>
            </h1>
          </div>
          <group name="studio_group_7b5162">
            <group name="studio_group_7b5162_left">
              <field name="x_studio_partner_id"/>
              <field name="x_studio_partner_phone" widget="phone" options="{'enable_sms': True}"/>
              <field name="x_studio_partner_email" widget="email"/>
              <field name="x_studio_date"/>
              <field name="x_studio_date_start" string="Dates" widget="daterange" options="{'end_date_field': 'x_studio_date_stop'}"/>
              <field name="x_studio_date_stop" invisible="1"/>
            </group>
            <group name="studio_group_7b5162_right">
              <field name="x_studio_user_id" widget="many2one_avatar_user"/>
              <field name="x_studio_currency_id" invisible="1"/>
              <field name="x_studio_value"/>
              <field name="x_studio_tag_ids" widget="many2many_tags" options="{'color_field': 'x_color'}"/>
            </group>
          </group>
          <group>
            <field name="x_studio_notes" placeholder="Type down your notes here..." nolabel="1" colspan="2"/>
          </group>
          <notebook>
            <page string="Details" name="lines">
              <field name="x_appointment_line_ids_cc2d6"/>
            </page>
          </notebook>
        </sheet>
        <chatter/>
      </form>
    </field>
    <field name="model">x_appointment</field>
    <field name="name">Default form view for x_appointment</field>
    <field name="type">form</field>
  </record>
  <record id="default_search_view__0713dd83-f3ac-453e-b72e-685ae5bc8356" model="ir.ui.view" context="{'studio': True}">
    <field name="arch" type="xml">
      <search>
        <field name="x_name"/>
        <field name="x_studio_partner_id" operator="child_of"/>
        <field name="x_studio_user_id"/>
        <field name="x_studio_tag_ids"/>
        <filter string="My Appointment" name="my_x_appointment" domain="[['x_studio_user_id', '=', uid]]"/>
        <separator/>
        <filter date="x_studio_date" name="studio_filter_date" string="Date"/>
        <filter date="x_studio_date_start" name="studio_filter_date_start" string="Start Date"/>
        <filter date="x_studio_date_stop" name="studio_filter_date_stop" string="End Date"/>
        <separator/>
        <filter string="Archived" name="archived_x_appointment" domain="[['x_active', '=', False]]"/>
        <separator/>
        <filter name="groupby_x_partner" string="Partner" context="{'group_by': 'x_studio_partner_id'}" domain="[]"/>
        <filter name="groupby_x_user" string="Responsible" context="{'group_by': 'x_studio_user_id'}" domain="[]"/>
        <filter name="x_studio_stage_id" string="Stage" context="{'group_by': 'x_studio_stage_id'}" domain="[]"/>
      </search>
    </field>
    <field name="model">x_appointment</field>
    <field name="name">Default search view for x_appointment</field>
    <field name="type">search</field>
  </record>
  <record id="default_calendar_vie_f711a5c5-1125-4823-b98f-5510642c3eab" model="ir.ui.view" context="{'studio': True}">
