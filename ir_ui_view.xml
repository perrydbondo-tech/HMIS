  <record id="default_kanban_view__20c3c051-e442-4921-9781-49f25adde1a3" model="ir.ui.view" context="{'studio': True}">
    <field name="arch" type="xml">
      <kanban default_group_by="x_studio_stage_id" default_order="x_studio_priority desc,x_studio_sequence asc,id desc" highlight_color="x_color">
        <field name="x_color"/>
        <field name="x_studio_currency_id"/>
        <progressbar field="x_studio_kanban_state" colors="{&quot;normal&quot;: &quot;200&quot;, &quot;done&quot;: &quot;success&quot;, &quot;blocked&quot;: &quot;danger&quot;}" sum_field="x_studio_value"/>
        <templates>
          <t t-name="menu">
            <a t-if="widget.editable" role="menuitem" type="open" class="dropdown-item">Edit</a>
            <a t-if="widget.deletable" role="menuitem" type="delete" class="dropdown-item">Delete</a>
            <field widget="kanban_color_picker" name="x_color"/>
          </t>
          <t t-name="card" class="flex-row">
            <aside>
              <field name="x_studio_image" widget="image" options="{&quot;zoom&quot;: true, &quot;background&quot;: true, &quot;preventClicks&quot;: false}"/>
            </aside>
            <main>
              <div name="studio_auto_kanban_header" class="d-flex">
                <field name="x_studio_priority" widget="boolean_favorite" nolabel="1"/>
                <field name="display_name" class="h5"/>
                <field name="x_studio_sequence" widget="handle" invisible="1"/>
              </div>
              <div name="studio_auto_kanban_body">
                <field name="x_studio_tag_ids" options="{'color_field': 'x_color'}"/>
              </div>
              <footer>
                <field name="x_studio_value"/>
                <div class="ms-auto d-flex align-items-center">
                  <field name="x_studio_kanban_state" widget="state_selection"/>
                  <field class="ms-1" name="x_studio_user_id" widget="kanban.many2one_avatar_user"/>
                </div>
              </footer>
            </main>
          </t>
        </templates>
      </kanban>
    </field>
    <field name="model">x_appointment</field>
    <field name="name">Default kanban view for x_appointment</field>
    <field name="type">kanban</field>
  </record>
